<struts>
	<package name="default" extends="struts-default">
	
		<!-- localhost:9080/Struts2App/getBasic.action -->
		<action name="getBasic" class="com.app.action.BasicAction">
		
			<!-- Path to jsp file is relative to /web directory -->
